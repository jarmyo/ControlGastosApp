<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ControlGastos.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ControlGastos.Mobile"
    Title="ControlGastos.Mobile">
    <ShellItem Title="Dashboard" Icon="dashboard.png">
        <ShellContent ContentTemplate="{DataTemplate local:DashboardPage}" />
    </ShellItem>
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
